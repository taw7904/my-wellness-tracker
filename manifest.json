{
    "name": "Wellness Tracker",
    "description": "Recieve daily check-in questions covering mental & physical wellness located right on your browser.",
    "version": "1.0",
    "permissions": ["activeTab", "tabs", "declarativeContent", "storage", "identity", "identity.email"],
    "background": {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/logo.png",
            "32": "images/logo.png",
            "48": "images/logo.png",
            "128": "images/logo.png"
        }
    },
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}


